# Graph for SIMS test report
<http://rdf.insee.fr/graphes/qualite/rapport/9999> {
    <http://bauhaus/qualite/rapport/9999>
            a                   sdmx-mm:MetadataReport ;
            rdfs:label          "Rapport qualite test"@fr , "Test quality report"@en ;
            sdmx-mm:target      <http://bauhaus/operations/operation/s9999> ;
            insee:validationState "Unpublished" .

    <http://bauhaus/qualite/attribut/9999/S.1.1>
            a                   sdmx-mm:ReportedAttribute ;
            simsv2fr:S.1.1      "Contact FR"@fr , "Contact EN"@en ;
            sdmx-mm:metadataReport <http://bauhaus/qualite/rapport/9999> .

    <http://bauhaus/qualite/attribut/9999/S.2.1>
            a                   sdmx-mm:ReportedAttribute ;
            simsv2fr:S.2.1      "2024-01-01"^^xsd:date ;
            sdmx-mm:metadataReport <http://bauhaus/qualite/rapport/9999> .

    <http://bauhaus/qualite/attribut/9999/S.10>
            a                   sdmx-mm:ReportedAttribute ;
            simsv2fr:S.10       <http://bauhaus/qualite/attribut/9999/S.10/texte> ;
            sdmx-mm:metadataReport <http://bauhaus/qualite/rapport/9999> .

    <http://bauhaus/qualite/attribut/9999/S.10/texte>
            a                   dctype:Text ;
            <http://www.w3.org/1999/02/22-rdf-syntax-ns#value>
                    "<p>Data description for test</p>"@fr ;
            dcterms:language    <http://bauhaus/codes/langue/fr> .

    # Document reference in SIMS
    <http://bauhaus/qualite/attribut/9999/S.7.1>
            a                   sdmx-mm:ReportedAttribute ;
            simsv2fr:S.7.1      <http://bauhaus/documents/document/9001> ;
            sdmx-mm:metadataReport <http://bauhaus/qualite/rapport/9999> .

    # Rubrics that should NOT be published (S.1.3 to S.1.8)
    <http://bauhaus/qualite/attribut/9999/S.1.3>
            a                   sdmx-mm:ReportedAttribute ;
            simsv2fr:S.1.3      "Internal contact info - should not be published"@fr ;
            sdmx-mm:metadataReport <http://bauhaus/qualite/rapport/9999> .

    # S.3.7 with organization reference - should be published
    <http://bauhaus/qualite/attribut/9999/S.3.7>
            a                   sdmx-mm:ReportedAttribute ;
            simsv2fr:S.3.7      <http://bauhaus/organisations/insee/DG75-F001> ;
            sdmx-mm:metadataReport <http://bauhaus/qualite/rapport/9999> .
}

